 InsertRow←{
     t←⍵
     ~t.InsertRows:0
     HandleInput t:0
     i j←t.ActiveCell
     n m←t.DataSize
     e←(0@i)1⍴⍨n+1
     c←t.Columns
     c.Value←e∘⍀¨c.Value
     c.Value←c.Value{v←⍺
         1=≢⍴v:v⊣v[i]←⊂⍵
         v⊣v[i;]←(1⊃⍴v)↑⍵
     }¨c.Default
     t.DataSize+←1 0
     t.Changed←1
     Refresh t
 }
