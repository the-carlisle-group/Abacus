 NewMenuBar←{
     ⍝ ⍺ ←→ Parent
     ⍝ ⍵ ←→ BuildMenu function
     f←1 A.FQP ⍵
     b←⍺ A.New'menu'
     b.BuildFunction←f
     b.class←'menu-bar'
     ⍝ b.Onclick←A.FQP'OnClickMenuBar'

     ⍝ !!!! We may not have a Document?
     ⍝ m←⍺.Document(⍎f)0
     m←(⍎f)0
     c←m.Content.Content
     i←1⊃¨(0⊃¨c.Content).Content
     l←b A.New¨{'li'⍵}¨i
     l.Onclick←⊂A.FQP'OnClickMenuBar'
     l.tabindex←0
     b
 }
